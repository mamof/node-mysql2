{
	"compilerOptions": {
		"target": "es2016",
		"alwaysStrict": true,
		"noUnusedLocals": true,
		"module": "commonjs",
		"rootDir": "src",
		"declaration": true,
		"strict": true,
		"strictFunctionTypes": false,
		"noImplicitAny": true,
		"sourceMap": true,
		"moduleResolution": "node",
		"removeComments": false,
		"listEmittedFiles": true,
		"outDir": "lib"
	}
} 